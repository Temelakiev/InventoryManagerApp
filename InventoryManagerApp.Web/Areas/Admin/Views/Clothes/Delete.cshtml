@model int

@{
    ViewData["Title"] = "Delete Clothe";
}

<h2>@ViewData["Title"]</h2>

<h3>Are you sure you want to delete this clothe?</h3>
<a asp-controller="Home" asp-action="index" class="btn btn-default">Back</a>
<a asp-area="Admin" asp-controller="Clothes" asp-action="Remove" asp-route-id="@Model" class="btn btn-danger">Confirm Delete</a>

